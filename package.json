{
    "name": "distortion-v0",
    "version": "1.0.0",
    "description": "distortion-v0",
    "main": "dist/index.js",
    "private": true,
    "scripts": {
        "up": "docker-compose up -d",
        "postup": "npm run start",
        "down": "docker-compose down",
        "start": "set TS_NODE_FILES=true && nodemon src/index.ts",
        "prebuild": "rd /S /q dist",
        "build": "npx tsc -p .",
        "lint": "eslint src --ext .ts --fix",
        "predocs": "rd /S /q \"src/docs\"",
        "docs": "apidoc -f \".*\\.ts$\" -i src/routes -o src/docs"
    },
    "keywords": [],
    "author": "nathandev",
    "license": "ISC",
    "engines": {
        "node": ">=16.11.0",
        "npm": ">=8.0.0"
    },
    "nodemonConfig": {
        "ignore": [
            "dist/**"
        ]
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/connect-redis": "0.0.18",
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.5",
        "@types/ioredis": "^4.28.10",
        "@types/mongoose": "^5.11.96",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/node": "16",
        "@types/nodemailer": "^6.4.6",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "apidoc": "^0.53.1",
        "eslint": "^8.25.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "connect-redis": "^6.1.3",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.6.0",
        "express-session": "^1.17.3",
        "ioredis": "^5.2.3",
        "joi": "^17.6.3",
        "mongo": "^0.1.0",
        "mongoose": "^6.6.5",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.8.0",
        "rate-limit-redis": "^3.0.1",
        "rate-limiter-flexible": "^2.3.11"
    }
}
